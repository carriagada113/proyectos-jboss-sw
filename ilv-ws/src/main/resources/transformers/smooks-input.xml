<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd"
                      xmlns:core="http://www.milyn.org/xsd/smooks/smooks-core-1.4.xsd"
                      xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.4.xsd">

    <core:namespaces>
        <core:namespace prefix="inputParamNS" uri="urn:cl.banchile.fsw:ilv-ws:1.0" />
    </core:namespaces>
   <core:exports>
      <core:result type="org.milyn.payload.JavaResult" extract="inputParam"/>
   </core:exports>
    <jb:bean beanId="inputParam" class="cl.banchile.fsw.ilv.ws.to.InputServicio" createOnElement="/inputParamNS:inputServicio">
        <jb:value property="rut" data="/inputParamNS:inputServicio/rut" />
    </jb:bean>

</smooks-resource-list>
