<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd"
                      xmlns:core="http://www.milyn.org/xsd/smooks/smooks-core-1.4.xsd"
                      xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.4.xsd">

    <core:namespaces>
        <core:namespace prefix="outputParamNS" uri="urn:cl.banchile.fsw:ilv-ws:1.0"/>
    </core:namespaces>
   <core:exports>
      <core:result type="org.milyn.payload.JavaResult" extract="outputParam"/>
   </core:exports>
   
   
    <jb:bean beanId="outputParam" class="cl.banchile.fsw.ilv.ws.to.OutputServicio" createOnElement="/outputParamNS:outputServicio">
        <jb:wiring beanIdRef="responseHeader" property="responseHeader" />
        <jb:wiring beanIdRef="responseBody" property="responseBody" />
    </jb:bean>

	<jb:bean beanId="responseHeader" class="cl.banchile.fsw.ilv.ws.to.ResponseHeader" createOnElement="/outputParamNS:outputServicio">
			<jb:value property="company" data="/outputParamNS:outputServicio/responseHeader/company" />
			<jb:value property="description" data="/outputParamNS:outputServicio/responseHeader/description" />
	</jb:bean>
	
    <jb:bean beanId="responseBody" class="cl.banchile.fsw.ilv.ws.to.ResponseBody" createOnElement="/outputParamNS:outputServicio">
        <jb:wiring beanIdRef="usuarios" property="usuarios" />
    </jb:bean>
    
    <jb:bean beanId="usuarios" class="java.util.ArrayList" createOnElement="/outputParamNS:outputServicio/responseBody/usuarios">
        <jb:wiring beanIdRef="usuarioTO" />
    </jb:bean>

	<jb:bean beanId="usuarioTO" class="cl.banchile.fsw.ilv.ws.to.UsuarioTO" createOnElement="/outputParamNS:outputServicio/responseBody/usuarios">
			<jb:value property="id" data="/outputParamNS:outputServicio/responseBody/usuarios/id" />
			<jb:value property="rut" data="/outputParamNS:outputServicio/responseBody/usuarios/rut" />
	        <jb:value property="username" data="/outputParamNS:outputServicio/responseBody/usuarios/username" />
	        <jb:value property="nombre" data="/outputParamNS:outputServicio/responseBody/usuarios/nombre" />
	        <jb:value property="apaterno" data="/outputParamNS:outputServicio/responseBody/usuarios/apaterno" />
	        <jb:value property="amaterno" data="/outputParamNS:outputServicio/responseBody/usuarios/amaterno" />
	</jb:bean>


</smooks-resource-list>