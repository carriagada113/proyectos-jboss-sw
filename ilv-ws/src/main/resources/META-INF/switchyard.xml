<?xml version="1.0" encoding="UTF-8"?>
<sy:switchyard xmlns:bean="urn:switchyard-component-bean:config:1.1" xmlns:sca="http://docs.oasis-open.org/ns/opencsa/sca/200912" xmlns:soap="urn:switchyard-component-soap:config:1.1" xmlns:sy="urn:switchyard-config:switchyard:1.1" xmlns:transform="urn:switchyard-config:transform:1.1" name="ilv-ws" targetNamespace="urn:cl.banchile.fsw:ilv-ws:1.0">
  <sca:composite name="ilv-ws" targetNamespace="urn:cl.banchile.fsw:ilv-ws:1.0">
    <sca:component name="ServicioBean">
      <bean:implementation.bean class="cl.banchile.fsw.ilv.ws.service.ServicioBean"/>
      <sca:service name="Servicio">
        <sca:interface.java interface="cl.banchile.fsw.ilv.ws.service.Servicio"/>
      </sca:service>
      <sca:property value="Banchile" name="company.name"/>
      <sca:property value="Servicio generado mediante arquetipo Maven." name="service.description"/>
    </sca:component>
    <sca:service name="Servicio" promote="ServicioBean/Servicio">
      <sca:interface.wsdl interface="Servicio.wsdl#wsdl.porttype(ServicioPortType)"/>
      <soap:binding.soap name="soap1">
        <soap:wsdl>Servicio.wsdl</soap:wsdl>
        <soap:wsdlPort>ServicioPort</soap:wsdlPort>
        <soap:contextPath>ilv-ws</soap:contextPath>
      </soap:binding.soap>
    </sca:service>
  </sca:composite>
  <sy:transforms>
    <transform:transform.smooks from="java:cl.banchile.fsw.ilv.ws.exception.UsuarioException" to="{urn:cl.banchile.fsw:ilv-ws:1.0}UsuarioException" type="JAVA2XML" config="transformers/smooks-output-fault.xml"/>
    <transform:transform.smooks from="java:cl.banchile.fsw.ilv.ws.to.OutputServicio" to="{urn:cl.banchile.fsw:ilv-ws:1.0}outputServicio" type="JAVA2XML" config="transformers/smooks-output-success.xml"/>
    <transform:transform.smooks from="{urn:cl.banchile.fsw:ilv-ws:1.0}inputServicio" to="java:cl.banchile.fsw.ilv.ws.to.InputServicio" type="XML2JAVA" config="transformers/smooks-input.xml"/>
  </sy:transforms>
</sy:switchyard>